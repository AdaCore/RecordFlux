   type Decimal_Values_With_Name_And_Description_Tag is
      (
       -- length = N, description = N/4 Quotes Server addresses, Ref: rfc2132
       --
       Quotes_Server => 8,

       -- length = N, description = N/4 Printer Server addresses, Ref: rfc2132
       --
       Lpr_Server => 9)
   with Size => 4, Always_Valid;

